    <!--
        SQL Statements
    -->
	<string-array name="sql_create_table_set" translatable="false">
		<item>CREATE TABLE topics (
			_id INTEGER PRIMARY KEY,
			topic_id INTEGER,
			title TEXT,
			url TEXT,
			content TEXT,
			content_rendered TEXT,
			replies INTEGER,
			time INTEGER
			);</item>

		<item>CREATE TABLE nodes (
			_id INTEGER PRIMARY KEY,
			node_id LONG,
			name TEXT,
			url TEXT,
			title TEXT,
			title_alternative TEXT,
			topics TEXT,
			header TEXT,
			footer TEXT,
			created TEXT
			);</item>
	</string-array>
